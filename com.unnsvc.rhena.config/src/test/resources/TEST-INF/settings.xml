	xsi:schemaLocation="urn:rhena:settings http://schema.unnsvc.com/rhena/settings.xsd">

	<repositories>
		<workspace>
			<repository location="file:/home/$USER/noname/.rhena/repository" />
		</workspace>
		<cache>
			<repository location="file:/path" />
		</cache>
		<remote>
			<repository location="http://some/location" />
		</remote>
	</repositories>

